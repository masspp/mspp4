<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane fx:id="pane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ninja.mspp.plugin.viewer.three_d.ThreeDPanel">
   <right>
      <Slider fx:id="slider" orientation="VERTICAL" BorderPane.alignment="CENTER" />
   </right>
   <top>
      <HBox minWidth="800.0" spacing="10.0" BorderPane.alignment="CENTER_LEFT">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <children>
            <ChoiceBox fx:id="themeChoice" prefWidth="300.0" />
            <CheckBox fx:id="axisCheck" mnemonicParsing="false" prefHeight="25.0" selected="true" text="Axis" />
            <CheckBox fx:id="rtCheck" mnemonicParsing="false" prefHeight="25.0" selected="true" text="RT" />
            <CheckBox fx:id="mzCheck" mnemonicParsing="false" prefHeight="25.0" selected="true" text="m/z" />
            <CheckBox fx:id="intCheck" mnemonicParsing="false" prefHeight="25.0" selected="true" text="Int." />
         </children>
      </HBox>
   </top>
</BorderPane>
